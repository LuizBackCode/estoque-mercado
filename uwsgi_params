uwsgi_params    QUERY_STRINGS       $query_strings;
uwsgi_params    REQUEST_METHOD      $request_method;
uwsgi_params    CONTEXT_TYPE        $context_type;
uwsgi_params    CONTEXT_LENGTH      $context_length;
uwsgi_params    REQUEST_URI         $request_uri;
uwsgi_params    PATH_INFO           $document_uri;
uwsgi_params    DOCUMENT_ROOT       $document_root;
uwsgi_params    SERVER_PROTOCOL     $server_protocol;
uwsgi_params    REQUEST_SCHEME      $scheme;
uwsgi_params    HTTPS               $https if_not_empty;
uwsgi_params    REMOTE_ADDR         $remote_addr;
uwsgi_params    REMOTE_PORT         $remote_port;
uwsgi_params    SERVER_PORT         $server_port;
uwsgi_params    SERVER_NAME         $server_name;